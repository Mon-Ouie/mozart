<!DOCTYPE BOOK SYSTEM "ozdoc.dtd" [
<!ENTITY % proglang.values.local "|sh|none">
<!ENTITY % p.mix.local "|em">
<!ELEMENT em - - (#PCDATA)>
<!ENTITY nbsp PI "nbsp">
<!ENTITY ellipsis PI "ellipsis">
<!ENTITY ie PI "ie">
<!ENTITY eg PI "eg">
<!entity amp "&">
<!entity etc PI "ellipsis">
<!entity chapter.src.get system "src-get.sgml">
<!entity chapter.src.make system "src-make.sgml">
<!entity section.configure system "configure.sgml">
]>
<book proglang=sh>
    <meta name="proglang.mode" arg1="sh" arg2="shell-script-mode">
    <meta name="proglang.mode" arg1="none" arg2="fundamental-mode">
    <meta name="entry.category" value="option">
    <meta name="entry.category" value="variable">
    <!-- split statements -->
    <meta name="html.split" value="chapter.src.get">
    <meta name="html.split" value="chapter.src.make">
    <title/Installation Manual/
    <author.extern to="author.db" key="Denys.Duchier">

    <part id=part.rpm>
      <title/RPM Distribution/
    </part>

    <part id=part.bin>
      <title/Binary Distribution/
    </part>

    <part id=part.src>
      <title/Source Distribution/

      &chapter.src.get;
      &chapter.src.make;

    </part>

</book>
