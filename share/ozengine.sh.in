#!/bin/sh

url=`@srcdir@/lib/cygpath.sh $1`
shift

case @PLATFORM@ in
    win32*)
        OZEMULATOR=`cygpath -w "$OZEMULATOR"`
        OZINIT=`cygpath -w "$OZINIT"`
        export OZEMULATOR OZINIT
        exec @BUILDTOP@/platform/mswindows/ozengine $url "$@"
        ;;
    *)
        exec $OZEMULATOR -u $url -- "$@"
        ;;
esac
