@SET_MAKE@
VPATH   = @scrdir@
SRCTOP  = @SRCTOP@
BUILDTOP= @BUILDTOP@
SUBDIRS = @SUBDIRS@
HOMEURL = @HOMEURL@

.PHONY: bootstrap all install clean distclean veryclean depend

all::

bootstrap all install clean veryclean distclean::
        dirs="$(SUBDIRS)"; \
        for i in $$dirs; do \
          if (cd $$i && $(MAKE) $@); \
          then true; \
          else exit 1; \
          fi; \
        done
clean veryclean distclean::
        -rm -f *~

distclean::
        -rm -f Makefile config.log config.status config.cache
