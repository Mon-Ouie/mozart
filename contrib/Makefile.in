@SET_MAKE@

BUILDTOP= @BUILDTOP@
PREFIX  = @prefix@
SUBDIRS = @CONTRIBDIRS@

.PHONY:	bootstrap all install clean distclean veryclean World files check

bootstrap::

bootstrap all install clean veryclean depend distclean::
	dirs="$(SUBDIRS)"; \
	for i in $$dirs; do \
	  if (cd $$i && $(MAKE) $@); \
          then true; \
          else exit 1; \
          fi; \
        done

clean veryclean distclean::
	-rm -f *~

distclean:: 
	-rm -f Makefile conf.all
	-rm -f config.log config.status config.cache

check::
