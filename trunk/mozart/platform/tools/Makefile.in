@SET_MAKE@
SUBDIRS = gump

.PHONY: all install clean veryclean depend distclean check
all::
all bootstrap install clean veryclean depend distclean check::
	dirs="$(SUBDIRS)"; \
	for i in $$dirs; do \
	  if (cd $$i && $(MAKE) $@); \
          then true; \
          else exit 1; \
          fi; \
        done

clean veryclean distclean:: local-clean
local-clean:
	-rm -f *~
distclean:: local-distclean
local-distclean:
	-rm -f Makefile config.*
