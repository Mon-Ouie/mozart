# The cpp used on this machine replaces all newlines and multiple tabs and
# spaces in a macro expansion with a single space.  Imake tries to compensate
# for this, but is not always successful.
#

OZHOME   = /usr/share/gs/soft/oz

SUBDIRS =

all::
	@ true

BINDIR  = $(OZHOME)/bin
SHELL   = /bin/sh

SCRIPTS = oz oz2lpr oz+ oz2declare ozarch
LINKS   = oz- ozc oz.compiler oz.machine ozwindowmanager

help:
	@echo ""
	@echo "The following targets are available"
	@echo "	install		{scheidhr,mehl}@dfki.uni-sb.de"
	@echo ""

install:
	install -m 555 $(SCRIPTS) $(OZHOME)/bin
	@set -x; cd $(BINDIR);\
	for file in $(LINKS); \
	       do\
	   rm -f $$file; \
	   ln -s oz $$file; \
	       done

CURRENTDIR=.

Makefile:: Imakefile

Makefile::
	@ if [ -f $@ ]; then /bin/mv -f $@ $@.bak; fi
	@ imake -DXX`ozarch` -I. -I.. -I../.. -I../../.. -I../../../..

all::   Makefile

clean::

depend::
	@ true

depends:: depend

World::
	make clean
	make depend
	make

Makefiles:: Makefile

