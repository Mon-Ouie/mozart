SHELL           = @SH@
LEX             = @LEX@
LFLAGS          = -8
YACC            = @YACC@
YFLAGS          = -d
MV              = @MV@
SED             = @SED@
ECHO            = @@ECHO@
M4              = @M4@
M4_S            = @M4_S@
RM              = @RM@ -f
CXX             = @CXX@
CXXCPP          = @CXXCPP@
LD              = @CXX@
DYNLD           = @DYNLD@
RANLIB          = @RANLIB@
AR              = @AR@

INSTALL         = @INSTALL@
INSTALL_DIR     = @MKINSTALLDIRS@
INSTALL_BIN     = $(INSTALL) -m 555
INSTALL_OTHER   = $(INSTALL) -m 444
LIBS            = @LIBS@
LDFLAGS         = @LDFLAGS@ $(CUSR)

DEFS            = @DEFS@
OZ_WARN         = @oz_warn@
OZ_WARN_ERROR   = @oz_warn_error@
CPPFLAGS        = @CPPFLAGS@ $(INCS) $(DEFS)
COPT            = @COPT@
CXXFLAGS        = $(COPT) $(OZ_WARN) $(OZ_WARN_ERROR) $(CUSR)
CXXFLAGS_NO_ERROR= $(COPT) $(OZ_WARN) $(CUSR)
DEPEND          = @DEPEND@ $(CPPFLAGS)

PREFIX          = @prefix@
PLATFORM        = @platform@
PLATFORMDIR     = $(PREFIX)/platform
BINDIR          = $(PLATFORMDIR)/$(PLATFORM)
INCDIR          = $(PREFIX)/include
